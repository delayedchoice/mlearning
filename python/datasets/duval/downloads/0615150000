     function openFeedback(re, sec) 
     {
         var loc = window.location.href.split('/').slice(0, -2).join('/');
         var url = loc + "/Feedback.aspx?RE=" + re + "&Section=" + sec;
         window.open(url, "", "location=no,height=800,width=800,resizable=no");
         //window.showModalDialog(url, "", 'dialogHeight:800px; dialogWidth:800px; resizable:1');
      }
 </script> 

<script type="text/javascript">
    window.downloadPDF = function (year, type) {
        var url = window.location.href.replace(/&pdfYear=([^&])*|&pdfType=([^&])*/ig, '');
        window.location = url + '&pdfYear=' + year + '&pdfType=' + type
    }
</script>



